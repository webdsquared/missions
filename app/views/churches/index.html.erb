<h1>Listing churches</h1>

<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>State</th>
    <th>Link</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @churches.each do |church| %>
  <tr>
    <td><%= church.name %></td>
    <td><%= church.city %></td>
    <td><%= church.state %></td>
    <td><%= church.link %></td>
    <td><%= link_to 'Show', church %></td>
    <td><%= link_to 'Edit', edit_church_path(church) %></td>
    <td><%= link_to 'Destroy', church, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Church', new_church_path %>
